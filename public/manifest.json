{
  "manifest_version": 3,
  "name": "Navinocode",
  "version": "0.0.1",
  "description": "A modern customizable start page with Apple-inspired design",
  "author": "Navinocode Team",
  "homepage_url": "https://github.com/y-shi23/Navinocode",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "https://source.unsplash.com/*",
    "https://api.github.com/*",
    "https://v1.jinrishici.com/*",
    "https://www.googleapis.com/*",
    "https://gstatic.com/*",
    "https://fonts.googleapis.com/*",
    "https://fonts.gstatic.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com data:; img-src 'self' data: https:; connect-src 'self' https://source.unsplash.com https://api.github.com https://v1.jinrishici.com"
  },
  "action": {
    "default_title": "Open Navinocode",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["assets/*", "fonts/*"],
      "matches": ["<all_urls>"]
    }
  ]
}